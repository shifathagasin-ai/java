import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
       int n;
        n=s.nextInt();
        if(n>0){
            System.out.print("Positive");
        }else{
            System.out.print("Negative");
        }
    
    }
}
